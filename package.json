{
  "name": "nomex",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node ./src/prisma/seed.ts",
    "schema": "./src/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:dev": "npx dotenv -e .env.local -- prisma migrate dev",
    "migrate:prod": "npx dotenv -e .env.local prisma migrate deploy",
    "db:push": "npx dotenv -e .env.local -- prisma db push",
    "migrate:reset": "npx dotenv -e .env.local -- prisma migrate reset",
    "db:seed": "npx dotenv -e .env.local -- ts-node src/prisma/seed.ts",
    "prisma:generate": "npx dotenv -e .env.local -- prisma generate",
    "prisma:studio": "npx dotenv -e .env.local -- prisma studio",
    "production:build": "npx prisma generate && npx prisma migrate deploy && next build",
    "postinstall": "npx prisma generate --schema=./src/prisma/schema.prisma",
    "cy:open": "cypress open",
    "component:tests": "cypress run --component",
    "e2e:tests": "cypress run --e2e"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.0",
    "@mui/material": "^5.13.5",
    "@prisma/client": "^4.16.2",
    "@types/bcrypt": "^5.0.0",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.5",
    "next": "13.4.5",
    "next-auth": "^4.22.1",
    "nextjs-cors": "^2.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.44.3",
    "styled-components": "^5.3.11",
    "swr": "^2.2.0"
  },
  "devDependencies": {
    "@types/next-auth": "^3.15.0",
    "@types/node": "^20.3.1",
    "@types/styled-components": "^5.1.26",
    "cypress": "^12.17.4",
    "prettier": "2.8.8",
    "prisma": "^4.16.1",
    "sass": "^1.63.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
